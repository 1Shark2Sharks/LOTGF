views:
  - type: table
    name: Character
    filters:
      and:
        - file.path.startsWith("LOTGF/Characters")
    order:
      - file.name
      - Status
      - Pathway
      - Sequence
      - Affiliation
      - "Former Affiliation:"
    sort:
      - property: file.basename
        direction: ASC
      - property: Status
        direction: ASC
    columnSize:
      note.Pathway: 287
